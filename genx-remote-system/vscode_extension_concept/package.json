{
  "name": "genx-agent-visualizer",
  "displayName": "GenX Agent Status Visualizer",
  "description": "A VS Code extension to visualize agent status and session data from your Firebase backend.",
  "version": "0.0.1",
  "publisher": "GenX",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:genx-agent-visualizer.showStatus"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "genx-agent-visualizer.showStatus",
        "title": "Show GenX Agent Status"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "typescript": "^4.8.4"
  }
}