# Jules Multi-Agent Communication Configuration
# Generated for GenX_FX Trading Platform

# Jules Communication Settings
JULES_HTTP_PORT=8080
JULES_API_HOST=localhost
JULES_API_URL=http://localhost:8080

# Multi-Agent Gateway Configuration
MULTI_AGENT_GATEWAY_PORT=8080
MULTI_AGENT_GATEWAY_HOST=0.0.0.0
MULTI_AGENT_GATEWAY_URL=http://localhost:8080

# Agent Communication Endpoints
AGENT_STATUS_ENDPOINT=/agent/status
AGENT_HEALTH_ENDPOINT=/health
AGENT_SIGNALS_ENDPOINT=/trading/signals
AGENT_ORDERS_ENDPOINT=/trading/orders

# Jules Plugin Configuration
JULES_PLUGIN_ENABLED=true
JULES_PLUGIN_PATH=./genx-cli/plugins/jules_plugin.js

# Network Configuration from network_map.md
GENX_CONTAINER_PORT=8080
REDIS_PORT=6379
MYSQL_PORT=3306
MONITORING_PORT=3001

# API Endpoints for Jules Communication
MT4_SIGNALS_URL=http://localhost:8080/MT4_Signals.csv
MT5_SIGNALS_URL=http://localhost:8080/MT5_Signals.csv
JSON_SIGNALS_URL=http://localhost:8080/signals/json
HEALTH_CHECK_URL=http://localhost:8080/health

# Authentication
DEVICE_AUTH_REQUIRED=true
JULES_DEVICE_ID=genx-fx-jules-agent

# Communication Protocol
COMMUNICATION_PROTOCOL=HTTP
COMMUNICATION_FORMAT=JSON
TIMEOUT_SECONDS=30
RETRY_ATTEMPTS=3

# Logging
JULES_LOG_LEVEL=INFO
JULES_LOG_FILE=./logs/jules_communication.log