# AMP System Environment Configuration Template
# Copy this file to .env and update with your actual API keys

# AMP Authentication
AMP_TOKEN=your_amp_token_here

# Trading Platform API Keys
FXCM_API_KEY=your_fxcm_api_key_here
FXCM_SECRET_KEY=your_fxcm_secret_key_here

# AI/ML API Keys
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# News and Data APIs
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
NEWS_API_KEY=your_news_api_key_here

# Database Configuration
POSTGRES_DB=amp_trading
POSTGRES_USER=amp_user
POSTGRES_PASSWORD=amp_password
POSTGRES_HOST=amp-postgres
POSTGRES_PORT=5432

# Redis Configuration
REDIS_HOST=amp-redis
REDIS_PORT=6379

# System Configuration
AMP_ENV=production
LOG_LEVEL=INFO
DEBUG=false

# Port Configuration
API_PORT=8000
GRAFANA_PORT=3000
